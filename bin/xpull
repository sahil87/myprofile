#!/bin/bash
OLDDIR=`pwd`
MYPROFILEDIR="$(realpath "$( cd "$( dirname "${BASH_SOURCE[0]}" )" && pwd )/.." )"
LIFETRACKERDIR="$(realpath "$( cd "$( dirname "${BASH_SOURCE[0]}" )" && pwd )/../../lifetracker" )"

cd $MYPROFILEDIR
git fetch origin
git merge origin/master
cd $LIFETRACKERDIR
git fetch origin
git merge origin/master
cd $OLDDIR
